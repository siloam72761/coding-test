    "N, M = map(int, input().split())\n",
    "\n",
    "A = list(map(int, input().split()))\n",
    "B = list(map(int, input().split()))\n",
    "\n",
    "A.sort()\n",
    "B.sort(reverse = True) #역순\n",
    "\n",
    "for i in range(M):\n",
    "    if A[i] < B[i]:\n",
    "        A[i], B[i]  = B[i], A[i]\n",
    "    else:\n",
    "        break\n",
    "\n",
    "print(sum(A))\n",
